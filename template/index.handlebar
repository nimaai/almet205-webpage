            <h3 class="section-subheading text-muted">{{prices-subtitle}}</h3>
          </div>
          <div class="row text-center">
            <div class="col-lg-12">
              <div class="table-responsive">
                <table class="table table-bordered table-striped">
                  <thead>
                    <tr>
                      <th></th>
                      <th>{{duration}}</th>
                      <th>{{whole-apartment}}</th>
                      <th>{{extra-bed}}</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <th class="text-nowrap" scope="row">15.12. - 31.3.</th>
                      <td>1-7 {{nights}}</td>
                      <td>50 EUR / {{night}}</td>
                      <td>5 EUR / {{night}}</td>
                    </tr>
                    <tr>
                      <th class="text-nowrap" scope="row">1.4.- 15.12.</th>
                      <td>1-7 {{nights}}</td>
                      <td>23 EUR / {{night}}</td>
                      <td>3 EUR / {{night}}</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
      </div>
      </div>
    </section>

